				float3 ase_worldBitangent = packedInput.ase_texcoord7.xyz;
				float3x3 ase_worldToTangent = float3x3(ase_worldTangent,ase_worldBitangent,ase_worldNormal);
				float3 ase_vertexBitangent = packedInput.ase_texcoord8.xyz;
				float3x3 objectToTangent = float3x3(packedInput.ase_tangent.xyz, ase_vertexBitangent, packedInput.ase_normal);
				float3 triplanar102 = TriplanarSampling102( _SnowNormal, packedInput.ase_texcoord9.xyz, packedInput.ase_normal, 1.0, _Vector0, 1.0, 0 );
				float3 tanTriplanarNormal102 = mul( objectToTangent, triplanar102 );
				float3 SnowNormal14 = tanTriplanarNormal102;
				float3 normalizedWorldNormal = normalize( ase_worldNormal );
				float temp_output_143_0 = ( _SnowAmount * TFW_SnowAmount );
				float temp_output_17_0 = ( normalizedWorldNormal.y * temp_output_143_0 );
				float3 lerpResult21 = lerp( Normal12 , SnowNormal14 , saturate( temp_output_17_0 ));
				float3 NormalBlend29 = lerpResult21;
				
				float2 uv_MetallicRSmoothnessA = packedInput.ase_texcoord3.xy * _MetallicRSmoothnessA_ST.xy + _MetallicRSmoothnessA_ST.zw;
